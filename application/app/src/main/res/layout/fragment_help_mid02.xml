<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".HelpFragmentMid02"
    android:background="#C5E1A5" >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="fragment_help_03"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/help_mid02_prev_button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="이전"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        android:background="@drawable/button_result"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/help_mid02_next_button"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:stateListAnimator="@null" />

    <Button
        android:id="@+id/help_mid02_next_button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="다음"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        android:background="@drawable/button_result"
        app:layout_constraintLeft_toRightOf="@id/help_mid02_prev_button"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="16dp"
        android:stateListAnimator="@null" />

</androidx.constraintlayout.widget.ConstraintLayout>